{
  "name": "docutils-ts",
  "version": "1.0.3",
  "description": "Port of the Python Docutils library to TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/boltex/docutils-ts.git"
  },
  "bugs": {
    "url": "https://github.com/boltex/docutils-ts/issues"
  },
  "homepage": "https://github.com/boltex/docutils-ts#readme",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core.js",
      "types": "./dist/core.d.ts"
    },
    "./languages": {
      "import": "./dist/languages/index.js",
      "types": "./dist/languages/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "build": "tsc"
  },
  "keywords": [
    "docutils",
    "typescript"
  ],
  "author": "FÃ©lix Malboeuf",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.3",
    "typescript": "^5.8.3"
  }
}
